#include <iostream>
#include <string>
using namespace std;
int main()
{
    string password;
    int tries;
    password = "endchiladas";
    cout << "Enter Password" << endl;
    string guess;
    cin >> guess;
    // "!=" means not equal
    while (guess != password && tries < 5){
        tries = tries + 1;
        cout << "wrong password" << endl << "try again";
        cin >> guess; //this line is to try again while it is not true
        
    }
    cout << "good boy ill let you in";
}
//ALWAYS FUCKING INCLUDE UNSING NAMESPACE STD;
