#include <stdio.h>
#include <iostream>
#include <string>
using namespace std;
int main()
{
    int i=1;
    while ( i <= 10) {
        cout << "current number: ";
        cout << i << endl;
        i = i+1;
    }
    //for looop
    for (int i=1; i != 10; i = i + 1){
    cout << "Current number with a forloop:";
    cout << i << endl;
    }
    return 0;
}
