#include <stdio.h>
#include <iostream>
#include <string>
using namespace std;
//ask for user name in lower case and print backwards but vertically and upper case
int main()
{
    char c;
    string name;
    cout << "what is ur name please enter all lower case" << endl;
    cin >> name;
    for (int i = 0; i < name.length(); i++){
        char capital = name[i] - 32;
        cout << capital << endl;
    
    }
}
