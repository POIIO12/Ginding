#include <stdio.h>
#include <iostream>
#include <string>
using namespace std;
int main()
{
    char c;
    // char variable is tp hold one single letter
    c = 'a'; //c has value 97
    while (c <= 'z'){
        if ( c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u'){
            char capital;
            capital = c - 32;
            cout << capital << endl;
        }
        else {
            cout << c << endl;
            
    }
    c = c + 1;
    }
    return 0;
}
